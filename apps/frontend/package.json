{
  "name": "@finance-tracker/frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-types": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:staging": "tsc -b && vite build --mode staging",
    "build:prod": "npm run clean && npm run type-check && npm run lint && tsc -b && vite build --mode production",
    "build:ssr": "npm run build && vite build --ssr",
    "preview": "vite preview",
    "preview:prod": "npm run build:prod && vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "analyze": "npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist .vite node_modules/.vite",
    "prebuild": "npm run clean",
    "validate": "npm run type-check && npm run lint && npm run test:coverage",
    "perf:lighthouse": "lhci autorun",
    "perf:bundle": "npm run build:analyze"
  },
  "dependencies": {
    "@finance-tracker/shared": "file:../../packages/shared",
    "@heroicons/react": "^2.2.0",
    "@types/node": "^24.2.1",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "finance-category-tracker": "file:../..",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.26.1",
    "recharts": "^3.1.2",
    "sentry": "^0.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/testing-library__jest-dom": "^5.14.9",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.1",
    "sharp": "^0.33.5",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.20.0",
    "vite-plugin-pwa": "^1.0.3"
  }
}
