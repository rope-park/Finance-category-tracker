{
  "name": "@finance-tracker/frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:staging": "tsc -b && vite build --mode staging",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "analyze": "npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist .vite node_modules/.vite",
    "prebuild": "npm run clean",
    "validate": "npm run type-check && npm run lint && npm run test:coverage"
  },
  "dependencies": {
    "@finance-tracker/shared": "file:../../packages/shared",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.2.1",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "finance-category-tracker": "file:../..",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.26.1",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.3",
    "vitest": "^1.6.1"
  }
}
